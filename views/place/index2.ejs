<h1>Places</h1>

<table class="table table-dark table-hover">
    <tr>
        <th>Name</th>
        <th>Email Address</th>
        <th>Phone Number</th>
        <th>Created At</th>
        <th>Updated At</th>
       
    </tr>


    <% place.forEach(function(place) { %>
        <tr>

           
            <!-- <td> <%= place.name %> </td> -->
            <td>
                <a href="/place/detail?id=<%= place._id %>"> 
                    <%= place.name %> 
                </a> 
            </td>
            <td> <%= place.emailAddress %> </td>
            <td> <%= place.phoneNumber %> </td>
            <td> <%= place.createdAt %> </td>
            <td> <%= place.updatedAt %> </td>
            <!-- <td><a href="/article/delete?id=<%= place._id %>">Delete</a></td> -->

    
        </tr>
    <% }) %>
    

</table>