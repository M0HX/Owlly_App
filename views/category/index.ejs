<h1> Categories </h1>
<table class="table-dark table-hover">
    <tr>
    <th>Title</th>
    <th>Delete</th>
    <th>Edit</th>    
    </tr>

  <% category.forEach(function(category){ %>

    <tr>
        <td> <%= category.name %> </td>
    
        <td> <a href="/category/delete?id=<%= category._id%>">Delete</a> </td>
        <td> <a href="/category/edit?id=<%= category._id%>">Edit</a> </td>
    </tr>
<% }) %>
</table>
