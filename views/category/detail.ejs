
<div >
    <div >
      <h2><%= category.name %></h2>
      <img class="divider" src="/images/divider.png">
    </div>
</div>
<div class="row">
   <% places.forEach(function (place){ %>  
     <div class="col-lg-3">
      <img class="bd-placeholder-img rounded-circle" width="180" height="180" src="<%= place.placeImg%>" role="img" aria-label="Image" preserveAspectRatio="xMidYMid slice" focusable="false"><title>Placeholder</title><rect width="100%" height="100%" fill="var(--bs-secondary-color)"></rect></svg>
      <a href="/place/detail?id=<%=place._id  %>">
      <h5 class="fw-normal"><%= place.name%></h5> </a>
      <% if(user) { %>
        <% if(user.isAdmin ) { %>
            <a href="/place/delete?id=<%= place._id %>">Delete</a>
            <a href="/place/edit?id=<%= place._id %>"> - Edit</a>
        <% } %>
      <% } %>  
  
     </div><!-- /.col-lg-4 -->

    <% }) %>

</div>